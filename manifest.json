{
  "manifest_version": 2,

  "name": "Tab Alphabetizer",
  "description": "Alphabetize your tabs by domain.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": [
      "alphabetize.js"
    ],
    "persistent": false
  },

  "permissions": [
    "tabs"
  ]
}

